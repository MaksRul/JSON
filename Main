import com.google.gson.Gson;

public class ParseJSON {
    public static void main(String[] args) {
        String pathJSON = "C:\\\\Users\\\\Максим\\\\Desktop\\\\JSONFile\\\\application/json ";
        Gson g = new Gson();
        Person person = g.fromJson(pathJSON,Person.class);
        System.out.println(person);

    }
}
